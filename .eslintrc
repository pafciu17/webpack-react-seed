{
  "extends": "eslint:recommended",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "react/jsx-uses-react" : 1
  },
  "globals": {
    "React": true
  }
}